<!DOCTYPE html>
<html>
  <head>
    <title>Foodiesocial</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <meta name="turbolinks-cache-control" content="no-cache" >

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'backend', 'data-turbolinks-track': 'reload' %>
  </head>


  <body>
    <%= render 'shared/backend/flash' if notice %>
    <%= render 'shared/backend/navbar' %>
    <section class="container">
      <%= yield %>
    </section>  
  </body>
</html>
