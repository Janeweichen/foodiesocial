<%= form_with(model:[:admin, category]) do |form| %>
  <div class="field">
    <%= form.label :name, '名稱', class: 'label'%>
    <%= form.text_field :name, class: 'input is-medium' %>
  </div>
    <%= link_to '回到上一頁', admin_categories_path, class: 'button is-grey' %>
